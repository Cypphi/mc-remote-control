	],
	"accessWidener": "mcrc.accesswidener",
	"depends": {
		"fabricloader": ">=0.17.2",
		"minecraft": "~1.21.10",
		"java": ">=21",
		"fabric-api": ">=0.134.1+1.21.10",
		"modmenu": ">=16.0.0-rc.1",
		"yet_another_config_lib_v3": ">=3.8.0+1.21.9"
